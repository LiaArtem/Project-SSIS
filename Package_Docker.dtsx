          For example, call MyOutputBuffer.AddRow() if your output was named "MyOutput".
        */

        JavaScriptSerializer js = new JavaScriptSerializer();
        var records = js.Deserialize<List<JSONData>>(jsonFileContent);

        NumberFormatInfo provider = new NumberFormatInfo { NumberDecimalSeparator = ".", NumberGroupSeparator = ","};
        double _forc = 100;

        foreach (var o in records)
        {            
            Выход0Buffer.AddRow();
            Выход0Buffer.r030 = o.Digitalcode;
            Выход0Buffer.txt = o.Currency_name;
            Выход0Buffer.rate = Convert.ToString(Convert.ToDouble(o.Official_hrivnya_exchange_rates, provider) / Convert.ToDouble(o.Unit, provider) * _forc);
            Выход0Buffer.cc = o.Lettercode;
            Выход0Buffer.exchangedate = o.Date;
            Выход0Buffer.forc = (int)_forc;
        }        
    }

}
